  Function {kitrefresh()} {} {
    code {//
        instrumentkitlist->copy_label(synth->getGuiMaster()->setPartWindowTitle("Kit List").c_str());
        drumMode->value(part->Pdrummode);
        kitMode->value(part->Pkitmode);
        if (kitMode->value())
        {
            partKitOn = true;
            kitlist->activate();
        }
        else
        {
            partKitOn = false;
            kitlist->deactivate();
        }
        lastkititem = -1;
        for (int i = 0; i < NUM_KIT_ITEMS; ++i)
        {
            if (part->kit[i].Penabled)
