  Function {~PartUI()} {} {
    code {//
    if (adnoteui)
        delete adnoteui;
    if (subnoteui)
        delete subnoteui;
    if (padnoteui)
        delete padnoteui;
    partgroup->hide();
    if (ATSeen)
        saveWin(synth, aftertouchcontrol->x(), aftertouchcontrol->y(), aftertouchcontrol->visible(), "afterT");
    aftertouchcontrol->hide();
    delete aftertouchcontrol;
    if (ctlSeen)
        saveWin(synth, ctlwindow->x(), ctlwindow->y(), ctlwindow->visible(), "ctlWin");
    ctlwindow->hide();
    delete ctlwindow;
    if (midiSeen)
        saveWin(synth, ctlmidi->x(), ctlmidi->y(), ctlmidi->visible(), "ctlmidi");
    ctlmidi->hide();
    delete ctlmidi;
    if (efxSeen)
        saveWin(synth, partfx->x(), partfx->y(), partfx->visible(), "partEfx");
    partfx->hide();
    delete partfx;
    if (kitSeen)
        saveWin(synth, instrumentkitlist->x(), instrumentkitlist->y(), instrumentkitlist->visible(), "partKit");
    instrumentkitlist->hide();
    delete instrumentkitlist;
    if (editSeen)
        saveWin(synth, instrumenteditwindow->x(), instrumenteditwindow->y(), instrumenteditwindow->visible(), "partEdit");
    instrumenteditwindow->hide();
    delete instrumenteditwindow;} {}
