  }
  Function {findengines(int npart)} {return_type {unsigned int}
  } {
    code {//
    unsigned int engine = 0;
    if (synth->getRuntime().checksynthengines)
    {
        for(int i = 0; i < NUM_KIT_ITEMS; ++i)
        {
            if (synth->part[npart]->kit[i].Padenabled)
                engine |= 1;
            if (synth->part[npart]->kit[i].Psubenabled)
                engine |= 2;
            if (synth->part[npart]->kit[i].Ppadenabled)
                engine |= 4;
        }
    }
    return engine;} {}
  }
  Function {setcolors(int engine)} {return_type int
  } {
    code {//
    int col;
    switch(engine)
    {
        case 1:
            col = ADD_COLOUR;
            break;
        case 2:
            col = SUB_COLOUR;
            break;
        case 4:
            col = PAD_COLOUR;
            break;
        default:
            col = BASE_COLOUR;
            break;
    }
    return col;} {}
  }
  Function {loadVector(string fname)} {} {
    code {//
    if (fname == "")
    {
        string lastname = synth->lastItemSeen(5);
        if (lastname == "")
            lastname = synth->getRuntime().userHome;
        char *filename;
        filename = fl_file_chooser("Open:", "({*.xvy})", lastname.c_str(), 0);
        if (filename == NULL)
            return;
        fname = string(filename);
    }
    send_data(84, 0, 0xf0, 240, 255, 255, BaseChan, 192, miscMsgPush(fname));} {}
  }
  Function {saveVector()} {} {
    code {//
    string lastname = synth->getLastfileAdded(5);
    if (lastname == "")
        lastname = synth->getRuntime().userHome;
    const char *file = NULL;
    file = fl_file_chooser("Save:", "({*.xvy})", lastname.c_str(), 0);
    if (file == NULL)
        return;

    string filename = setExtension(string(file), "xvy");
    if (isRegFile(filename))
        if (fl_choice("The file exists. \\nOverwrite it?", NULL, "No", "Yes") < 2)
            return;
    int pos = loadlabel[BaseChan].find("No Name");
    if (pos >=0 && pos < 2)
        Loaded->copy_label(findleafname(filename).c_str());
    send_data(85, 0, 0xc0, 240, 255, 255, BaseChan, 128, miscMsgPush(filename));} {}
  }
  Function {clearVector(int BaseC)} {} {
    code {//
    int min = 0;
    int max = NUM_MIDI_CHANNELS;
    if(BaseC < NUM_MIDI_CHANNELS)
    {
        min = BaseC;
        max = BaseC + 1;
    }
    for (int i = min; i < max; ++i)
    {
        loadlabel[i] = "No Name " + to_string(i + 1);
        Loaded->copy_label(loadlabel[i].c_str());
     }
     if (BaseC >= NUM_MIDI_CHANNELS)
     {
         BaseChan = 0;
         setInstrumentLabel(0);
         setInstrumentLabel(NUM_MIDI_CHANNELS);
         setInstrumentLabel(NUM_MIDI_CHANNELS * 2);
         setInstrumentLabel(NUM_MIDI_CHANNELS * 3);
     }

     Xcc = 0;
     Xf = 0;
     Xgroup->deactivate();
     Xcontrol->value(0);
     Ycc = 0;
     Yf = 0;
     Ygroup->deactivate();
     Ycontrol->deactivate();
     Ycontrol->value(0);} {}
  }
  Function {errorlabel(string msg)} {} {
    code {//
            synth->getGuiMaster()->words->copy_label(msg.c_str());
            synth->getGuiMaster()->message->show();
            synth->getGuiMaster()->message->position(Fl::event_x_root() + 16, Fl::event_y_root());} {}
  }
