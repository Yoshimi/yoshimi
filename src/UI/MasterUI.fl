  Function {updatepart()} {} {
    code {//
    if (checkmaxparts())
    {
        if (npart >= partmax)
        {
            npart %= NUM_MIDI_CHANNELS;
            npart += (partmax - NUM_MIDI_CHANNELS);
            npartcounter->value(npart + 1);
            npartcounter->do_callback();
        }
        maxparts->value(partmax);
        maxparts->redraw();
        return;
    }

    partEnable->value(synth->partonoffRead(npart));
    maxparts->value(partmax);
    maxparts->redraw();
    npartcounter->value(npart + 1);
    npartcounter->redraw();
    refreshControls(npart);} {}
