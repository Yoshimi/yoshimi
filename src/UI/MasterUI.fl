  Function {send_data(int control, float value, int type, int part = 0xff, int engine = 0xff, int insert = 0xff, int parameter = 0xff, int par2 = 0xff)} {} {
    code {//
    if (parameter == 0)
    {
        collect_data(synth, 0, (Fl::event_button() | type), control, part, 0xff, 0xff, 0xff, 0xff, par2);
        return;
    }
    if (part == 0xff)
        part = 0xf0;

    collect_data(synth, value, (Fl::event_button() | type), control, part, 0xff, engine, insert, parameter, par2);} {}
  }
  Function {returns_update(CommandBlock *getData)} {} {
    code {//
    float value = getData->data.value;
    bool wasFromHere = getData->data.type & 0x20;
    unsigned char control = getData->data.control;
    unsigned char npart = getData->data.part;
    unsigned char engine = getData->data.engine;
    unsigned char insert = getData->data.insert;
    unsigned char par2 = getData->data.par2;
    int nval = lrint(value);
    if (npart == 0xf1)
    {
        if (insert == 16)
        {
            if (!wasFromHere)
                syseffsend[engine][control]->value(value);
        }
        else
        {
            if (control == 0)
            {
                syseffnocounter->value(engine + 1);
                nsyseff = engine;
                sysefftype->value(synth->sysefx[nsyseff]->geteffect());
                syseffectui->refresh(synth->sysefx[nsyseff], 0xf1, nsyseff);
            }
            else if (control == 1)
            {
                syseffnocounter->value(engine + 1);
                nsyseff = engine;
                sysefftype->value(nval);
                syseffectui->refresh(synth->sysefx[nsyseff], 0xf1, nsyseff);
                showSysEfxUI();
            }
        }
        return;
    }
    else if (npart == 0xf2)
    {
        if (control == 0)
        {
            ninseff = engine;
            setInsEff(ninseff);
        }
        else if (control == 1)
        {
            insefftype->value(nval);
            inseffectui->refresh(synth->insefx[nsyseff], 0xf1, ninseff);
            showInsEfxUI();
        }
        else if (control == 2)
        {
            inseffpart->value(nval + 2);
            if (value == -1)
                insefftype->deactivate();
            else
                insefftype->activate();
        }
        return;
    }
    string name;

    switch(control)
    {
        case 0:
            mastervolumedial->value(value);
            break;

        case 14:
            partuigroup->remove(partui);
            delete partui;
            partui = new PartUI(0, 0, 765, 525);
            partuigroup->add(partui);
            partui->init(synth->part[nval], nval, bankui);
            partui->redraw();
            npart = nval;
            npartcounter->value(nval + 1);
            npartcounter->redraw();
            updatepanel();
            break;

        case 15:
            partmax = nval;
            npartcounter->range(1, partmax);
            updatepart();
            updatepanel();
            setinspartlist();
            break;

        case 32:
            globalfinedetune->value(value);
            break;

        case 35:
            masterkeyshift->value(value);
            break;

        case 48:
        case 49:
            updatepanel();
            break;

        case 80:
            name = miscMsgPop(par2);
            if (name.find("FAILED") == 1)
            {
                if (wasFromHere)
                    fl_alert("%s",name.c_str());
            }
            else
            {
                setMasterLabel(findleafname(name));
                refresh_master_ui(0);
            }

            break;

        case 81: // save patch set
            name = miscMsgPop(par2);
            if (name.find("FAILED") == 1)
            {
                if (wasFromHere)
                    fl_alert("%s",name.c_str());
            }
            else
            {
                setMasterLabel(findleafname(name));
                RecentParams->activate();
                updatepanel();
            }
            break;

        case 84: // load vector
            name = miscMsgPop(par2);
            if (name.find("FAILED") == 1)
            {
                if (wasFromHere)
                    fl_alert("%s",name.c_str());
            }
            else
            {
                vectorui->setLoadLabelRemote(insert, synth->getRuntime().vectordata.Name[insert]);
                vectorui->recenthistory->activate();
                refresh_master_ui(2);
            }
            break;

        case 85: // save vector
            name = miscMsgPop(par2);
            if (name.find("FAILED") == 1)
            {
                if (wasFromHere)
                    fl_alert("%s",name.c_str());
            }
            else
                vectorui->recenthistory->activate();
            break;

        case 88: // load scales
            name = miscMsgPop(par2);
            if (name.find("FAILED") == 1)
            {
                if (wasFromHere)
                    fl_alert("%s",name.c_str());
            }
            else
            {
                microtonalui->Hide();
                microtonalui->make_window();
            }
            break;

        case 89: // save scales
            name = miscMsgPop(par2);
            if (wasFromHere && name.find("FAILED") == 1)
                    fl_alert("%s",name.c_str());
            break;

        case 92: // load state
            name = miscMsgPop(par2);
            if (wasFromHere && name.find("FAILED") == 1)
                fl_alert("%s",name.c_str());
            refresh_master_ui(0);
            break;

        case 93: // save state
            name = miscMsgPop(par2);
            if (wasFromHere && name.find("FAILED") == 1)
                fl_alert("%s",name.c_str());
            break;

        case 96:
            if (npart == 240)
                refresh_master_ui(1);
            else if (npart < 64)
                partNew(npart);
            break;

        case 128:
            virkeyboard->relaseallkeys();
            vumeter->init(-1, synth);
            for(int i = 0; i < NUM_MIDI_CHANNELS; ++i)
            {
                if(panelwindow)
                {
                    if(panellistitem[i]->partVUMeter)
                    panellistitem[i]->partVUMeter->resetPart();
                }
            }
            break;
    }} {}
  }
