        putData("kitlist",
                 partui->instrumentkitlist->x(),
                 partui->instrumentkitlist->y(),
                 partui->instrumentkitlist->visible());
    else
        putData("kitlist",
                 kitlistX,
                 kitlistY,
                 kitlistO);

    if(partui->partfx->x())
        putData("partFX",
                 partui->partfx->x(),
                 partui->partfx->y(),
                 partui->partfx->visible());
    else
        putData("partFX",
                 partfxX,
                 partfxY,
                 partfxO);

    putData("SUBnote",
             subNoteX,
             subNoteY,
             subNoteO);
    putData("PADnote",
             padNoteX,
             padNoteY,
             padNoteO);
    putData("ADDnote",
             addNoteX,
             addNoteY,
             addNoteO);

   fclose(windows);} {}
  }
  Function {getData(bool ok, string name, int& X, int& Y, int& O)} {return_type bool
  } {
    code {//
    if (NewWindows)
    {
        X = 1;
        Y = 1;
        O = 0;
    }
    if (!ok)
        return false;

    ok = (fgets(word, 20, windows) != NULL);
    if (ok)
        ok = strstr(word, name.c_str());
    if (true)
    {
        ok = false;
        if (fgets(word, 20, windows) != NULL)
            X = atoi(word);
        if (fgets(word, 20, windows) != NULL)
            Y = atoi(word);
        if (fgets(word, 20, windows) != NULL)
        {
            O = atoi(word);
            ok = true;
        }
    }
    return ok;} {}
  }
