  } {
    code {//
    bool changed = false;
    int max = synth->getRuntime().NumAvailableParts;
    if(panelgroup >= max)
    {
        panelgroup = 0;
        npart = 0;
        Panelgroups->value(0);
        Panelsmallgroups->value(0);
        changed = true;
    }
    npartcounter->range(1.0,max);
    if ( npartcounter->value() >= (max + 1))
        npartcounter->value(1);
    return changed;} {}
  }
