  Function {returns_update(CommandBlock *getData)} {} {
    code {//
    float value = getData->data.value;
    int value_int = int(value + 0.5f);
    unsigned char control = getData->data.control;
    unsigned char efftype = getData->data.kit;
    unsigned char effnum = getData->data.engine;
    bool isdynfilter = (effnum == (EFFECT::type::dynFilter));
    unsigned char eqband =  getData->data.parameter;
    unsigned char secondary = getData->data.offset;
    if (efftype != EFFECT::type::eq)
        presetNum = eff->getpreset();

    int textCol = eff_preset;
    if (fetchData(0, EFFECT::control::changed, getData->data.part, efftype, effnum) != 0)
        textCol = eff_preset_changed;
    //std::cout << "colour " << textCol << std::endl;
    switch(efftype)
    {
        case EFFECT::type::none:
            break;

        case EFFECT::type::reverb:
            switch (control)
            {
                case EFFECT::control::level:
                    revp0->value(value);
                    if (eff->insertion != 0) // D/W
                        revp0->selection_color(setKnob(value * 2,reverbPresets[presetNum][0]));
                    else
                        revp0->selection_color(setKnob(value,reverbPresets[presetNum][0]));
                    break;
                case EFFECT::control::panning:
                    revp1->value(value);
                    revp1->selection_color(setKnob(value,reverbPresets[presetNum][1]));
                    break;
                case EFFECT::control::frequency: // time here
                    revp2->value(value);
                    revp2->selection_color(setKnob(value,reverbPresets[presetNum][2]));
                    break;
                case 3:
                    revp3->value(value);
                    revp3->selection_color(setKnob(value,reverbPresets[presetNum][3]));
                    break;
                case 4:
                    revp4->value(value);
                    revp4->selection_color(setKnob(value,reverbPresets[presetNum][4]));
                    break;
                case 6:
                    revp6->value(value);
                    revp6->selection_color(setKnob(value,reverbPresets[presetNum][6]));
                    break;
                case 7:
                    revp7->value(value);
                    revp7->selection_color(setKnob(value,reverbPresets[presetNum][7]));
                    break;
                case 8:
                    revp8->value(value);
                    revp8->selection_color(setKnob(value,reverbPresets[presetNum][8]));
                    break;
                case 9:
                    revp9->value(value);
                    revp9->selection_color(setKnob(value,reverbPresets[presetNum][9]));
                    break;
                case 10:
                    revp10->value(value_int);
                    if (value_int == 2)
                    {
                        revp12->activate();
                        revp12->value(secondary);
                    }
                    else
                        revp12->deactivate();
                    break;
                case 11:
                    revp11->value(value);
                    revp11->selection_color(setKnob(value,reverbPresets[presetNum][11]));
                    break;
                case 12:
                    revp12->value(value);
                    revp12->selection_color(setKnob(value,reverbPresets[presetNum][12]));
                    break;
                case EFFECT::control::preset:
                    refresh();
                    break;
            }
            revp->textcolor(textCol);
            revp->redraw();
            break;

        case EFFECT::type::echo:
            switch (control)
            {
                case EFFECT::control::level:
                    echop0->value(value);
                    if (eff->insertion != 0) // D/W
                        echop0->selection_color(setKnob(value,int(echoPresets[presetNum][0])/2));
                    else
                        echop0->selection_color(setKnob(value,echoPresets[presetNum][0]));
                    break;
                case EFFECT::control::panning:
                    echop1->value(value);
                    echop1->selection_color(setKnob(value,echoPresets[presetNum][1]));
                    break;
                case EFFECT::control::frequency: // delay here
                    echop2->value(value);
                    echop2->selection_color(setKnob(value,echoPresets[presetNum][2]));
                    break;
                case 3:
                    echop3->value(value);
                    echop3->selection_color(setKnob(value,echoPresets[presetNum][3]));
                    break;
                case 4:
                    echop4->value(value);
                    echop4->selection_color(setKnob(value,echoPresets[presetNum][4]));
                    break;
                case 5:
                    echop5->value(value);
                    echop5->selection_color(setKnob(value,echoPresets[presetNum][5]));
                    break;
                case 6:
                    echop6->value(value);
                    echop6->selection_color(setKnob(value,echoPresets[presetNum][6]));
                    break;
                case EFFECT::control::bpm:
                    echop17->value(value);
                    if (value)
                        echop2->setValueType(VC_FXlfofreqBPM);
                    else
                        echop2->setValueType(VC_FXEchoDelay);
                    break;
                case EFFECT::control::preset:
                    refresh();
                    break;
            }
            echop->textcolor(textCol);
            echop->redraw();
            break;

        case EFFECT::type::chorus:
            switch (control)
            {
                case EFFECT::control::level:
                    chorusp0->value(value);
                    chorusp0->selection_color(setKnob(value,chorusPresets[presetNum][0]));
                    break;
                case EFFECT::control::panning:
                    chorusp1->value(value);
                    chorusp1->selection_color(setKnob(value,chorusPresets[presetNum][1]));
                    break;
                case EFFECT::control::frequency:
                    chorusp2->value(value);
                    chorusp2->selection_color(setKnob(value,chorusPresets[presetNum][2]));
                    break;
                case 3:
                    chorusp3->value(value);
                    chorusp3->selection_color(setKnob(value,chorusPresets[presetNum][3]));
                    break;
                case 5:
                    chorusp5->value(value);
                    chorusp5->selection_color(setKnob(value,chorusPresets[presetNum][5]));
                    break;
                case 6:
                    chorusp6->value(value_int);
                    chorusp6->selection_color(setKnob(value,chorusPresets[presetNum][6]));
                    break;
                case 7:
                    chorusp7->value(value);
                    chorusp7->selection_color(setKnob(value,chorusPresets[presetNum][7]));
                    break;
                case 8:
                    chorusp8->value(value);
                    chorusp8->selection_color(setKnob(value,chorusPresets[presetNum][8]));
                    break;
                case 9:
                    chorusp9->value(value);
                    chorusp9->selection_color(setKnob(value,chorusPresets[presetNum][9]));
                    break;
                case 11:
                    chorusp11->value(value_int);
                    break;
                case EFFECT::control::bpm:
                    chorusp17->value(value);
                    if (value)
                    {
                        chorusp2->setValueType(VC_FXlfofreqBPM);
                        chorusp18->activate();
                    }
                    else
                    {
                        chorusp2->setValueType(VC_FXlfofreq);
                        chorusp18->deactivate();
                    }
                    break;
                case EFFECT::control::bpmStart:
                    chorusp18->value(value);
                    chorusp18->selection_color(setKnob(value,0));
                    break;
                case EFFECT::control::preset:
                    refresh();
                    break;
            }
            chorusp->textcolor(textCol);
            chorusp->redraw();
            break;

        case EFFECT::type::phaser:
            switch (control)
            {
                case EFFECT::control::level:
                    phaserp0->value(value);
                    phaserp0->selection_color(setKnob(value,phaserPresets[presetNum][0]));
                    break;
                case EFFECT::control::panning:
                    phaserp1->value(value);
                    phaserp1->selection_color(setKnob(value,phaserPresets[presetNum][1]));
                    break;
                case EFFECT::control::frequency:
                    phaserp2->value(value);
                    phaserp2->selection_color(setKnob(value,phaserPresets[presetNum][2]));
                    break;
                case 3:
                    phaserp3->value(value);
                    phaserp3->selection_color(setKnob(value,phaserPresets[presetNum][3]));
                    break;
                case 4:
                    phaserp4->value(value_int);
                    break;
                case 5:
                    phaserp5->value(value);
                    phaserp5->selection_color(setKnob(value,phaserPresets[presetNum][5]));
                    break;
                case 6:
                    phaserp6->value(value);
                    phaserp6->selection_color(setKnob(value,phaserPresets[presetNum][6]));
                    break;
                case 7:
                    phaserp7->value(value);
                    phaserp7->selection_color(setKnob(value,phaserPresets[presetNum][7]));
                    break;
                case 8:
                    phaserp8->value(value_int);
                    break;
