            effdynamicfilterwindow->show();
            break;
        default:
            effnullwindow->show();
            break;
    }
    lastEff = nextEff;
    this->show();
    //if (nextEff != EFFECT::type::none)
    {
        //effRtext(this->w()/380, nextEff - EFFECT::type::none);
    }} {}
