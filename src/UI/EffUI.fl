        case 7:
            eqband = 0;
            eqp0->value(eff->geteffectpar(0));
            bandcounter->value(eqband);
            typechoice->value(eff->geteffectpar(10));
            eqgraph->redraw();
            freqdial->value(eff->geteffectpar(11));
