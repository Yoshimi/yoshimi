  Function {refresh(EffectMgr *eff_, int npart_, int neff_)} {} {
    code {//
    eff = eff_;
    npart = npart_;
    neff = neff_;
    this->hide();
    effnullwindow->hide();
    effreverbwindow->hide();
    effechowindow->hide();
    effchoruswindow->hide();
    effphaserwindow->hide();
    effalienwahwindow->hide();
    effdistorsionwindow->hide();
    effeqwindow->hide();
