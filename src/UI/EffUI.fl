                        dfp18->deactivate();
                    }
                    break;
                case EFFECT::control::bpmStart:
                    dfp18->value(value);
                    dfp18->selection_color(setKnob(value,0));
                    break;
                case EFFECT::control::preset:
                    refresh();
                    break;
            }
            dfp->textcolor(textCol);
            dfp->redraw();
            break;
    };} {}
