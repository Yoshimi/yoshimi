            if (eff->geteffectpar(10) < 6)
                gaindial->deactivate();
            qdial->value(eff->geteffectpar(13));
            stagescounter->value(eff->geteffectpar(14));
            eqgraph->init(eff);
            effeqwindow->show();
            break;
        case 8:
            make_filter_window();
            dfp->value(eff->getpreset());
            dfp0->value(eff->geteffectpar(0));
            if (eff->insertion!=0)
