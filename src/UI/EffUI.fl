            if (type < 6)
                gaindial->deactivate();
            stagescounter->value(eff->geteffectpar(14));
            eqgraph->init(eff);
            eqgraph->damage(1);
            eqgraph->redraw();
            effeqwindow->show();
            break;
        }
        case EFFECT::type::dynFilter:
            make_filter_window();
            dfp->value(eff->getpreset());
            dfp0->value(eff->geteffectpar(0));
            if (eff->insertion!=0)
            {
                dfp0->label("D/W");
                dfp0->selection_color(setKnob(dfp0->value(),int(dynPresets[presetNum][0])));
            }
            else
                dfp0->selection_color(setKnob(dfp0->value(),int(dynPresets[presetNum][0])/2));
            dfp1->value(eff->geteffectpar(1));
            dfp1->selection_color(setKnob(dfp1->value(),int(dynPresets[presetNum][1])));
            dfp2->value(eff->geteffectpar(2));
            dfp2->selection_color(setKnob(dfp2->value(),int(dynPresets[presetNum][2])));
            dfp3->value(eff->geteffectpar(3));
            dfp3->selection_color(setKnob(dfp3->value(),int(dynPresets[presetNum][3])));
            dfp4->value(eff->geteffectpar(4));
            dfp4->selection_color(setKnob(dfp4->value(),int(dynPresets[presetNum][4])));
            dfp5->value(eff->geteffectpar(5));
            dfp5->selection_color(setKnob(dfp5->value(),int(dynPresets[presetNum][5])));
            dfp6->value(eff->geteffectpar(6));
            dfp6->selection_color(setKnob(dfp6->value(),int(dynPresets[presetNum][6])));
            dfp7->value(eff->geteffectpar(7));
            dfp7->selection_color(setKnob(dfp7->value(),int(dynPresets[presetNum][7])));
            dfp8->value(eff->geteffectpar(8));
            dfp9->value(eff->geteffectpar(9));
            dfp9->selection_color(setKnob(dfp9->value(),int(dynPresets[presetNum][9])));
            dfp17->value(eff->geteffectpar(17));
            if (dfp17->value())
            {
                dfp2->setValueType(VC_FXlfofreqBPM);
                dfp18->activate();
            }
            else
            {
                dfp2->setValueType(VC_FXlfofreq);
                dfp18->deactivate();
            }
            dfp18->value(eff->geteffectpar(18));
            dfp18->selection_color(setKnob(dfp1->value(),0));
            effdynamicfilterwindow->show();
            break;
        default:
            effnullwindow->show();
            break;
    }
    lastEff = nextEff;
    this->show();} {}
  }
